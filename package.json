{
  "name": "showfinder",
  "version": "1.0.0",
  "description": "Local Concert Search Tool",
  "scripts": {
    "start": "concurrently \"cd backend && npm run dev\" \"cd frontend && PORT=3000 npm start\"",
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "license": "MIT",
  "dependencies": {
    "all": "^0.0.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.7",
    "react-router-dom": "^7.1.5"
  }
}
